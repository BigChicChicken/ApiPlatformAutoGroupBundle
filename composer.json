{
    "name": "bigchicchicken/api-platform-auto-group-bundle",
    "type": "api-platform-bundle",
    "description": "Bundle to define automatically all groups on all entities for ApiPlatform",
    "keywords": ["symfony", "api-patform", "groups"],
    "homepage": "https://github.com/BigChicChicken/ApiPlatformAutoGroupBundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ApiPlatformAutoGroupBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiPlatformAutoGroupBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Florent TEDESCO"
        }
    ],
    "require": {
        "php": ">=8.1",
        "api-platform/core": "^3.0",
        "symfony/dependency-injection": "^6.1",
        "symfony/framework-bundle": "^6.1",
        "symfony/http-kernel": "^6.1",
        "symfony/yaml": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.1"
    }
}
